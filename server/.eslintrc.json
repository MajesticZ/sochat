{
    "extends": "airbnb-base",
    "rules": {
        "max-len": ["error", { "code": 180, "tabWidth": 2 }],
        "no-plusplus": ["off"],
        "comma-dangle": ["off"],
        "no-unused-vars": ["error"],
        "no-console": ["warn"],
        "arrow-parens": ["error", "always"],
        "object-curly-spacing": ["off"],
        "padded-blocks": ["off"]
    }
}